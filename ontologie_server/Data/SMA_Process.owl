    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://siemens.com/knowledge_graph/cyber_physical_systems/sma/process#InsertBlockByEnhancedPortalRobot"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2000/01/rdf-schema#subClassOf"/>
