                                                                <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">180</owl:hasValue>
                                                            </owl:Restriction>
                                                        </owl:unionOf>
                                                    </owl:Class>
                                                    <owl:Restriction>
                                                        <owl:onProperty rdf:resource="http://siemens.com/knowledge_graph/industrial_upper_ontology#connects"/>
                                                        <owl:someValuesFrom>
                                                            <owl:Restriction>
